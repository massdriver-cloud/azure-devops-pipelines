{
  "manifestVersion": 1,
  "id": "mass-cli",
  "name": "Mass CLI",
  "version": "0.0.1",
  "publisher": "Massdriver",
  "public": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/massdriver-cloud/azure-devops-pipelines"
  },
  "icons": {
    "default": "icon.png"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Mass CLI extension for Azure DevOps Pipeline",
  "categories": ["Azure Pipelines"],
  "tags": [
    "Massdriver",
    "CI/CD",
    "Azure",
    "AWS",
    "GCP",
    "DevOps",
    "IDP",
    "Platform Engineering"
  ],
  "content": {
    "details": {
      "path": "overview.md"
    }
  },
  "links": {
    "demo": {
      "uri": "https://www.massdriver.cloud/demo",
      "text": "Book a Demo"
    },
    "learn": {
      "uri": "https://docs.massdriver.cloud",
      "text": "Learn about Massdriver"
    },
    "slack": {
      "uri": "https://join.slack.com/t/massdrivercommunity/shared_invite/zt-1sxag35w2-eYw7gatS1hwlH2y8MCmwXA",
      "text": "Community Slack"
    },
    "privacy": {
      "uri": "https://www.massdriver.cloud/privacy-policy",
      "text": "Privacy Policy"
    }
  },
  "files": [
    {
      "path": "massInstallCLI"
    },
    {
      "path": "massBundlePublish"
    }, 
    {
      "path": "massImagePush"
    },
    {
      "path": "massAppPatch"
    },
    {
      "path": "massAppDeploy"
    } 
  ],
  "contributions": [
    {
      "id": "mass-cli-install",
      "type": "ms.vss-distributed-task.task",
      "targets": ["ms.vss-distributed-task.tasks"],
      "properties": {
        "name": "massInstallCLI"
      }
    },
    {
      "id": "mass-bundle-publish",
      "type": "ms.vss-distributed-task.task",
      "targets": ["ms.vss-distributed-task.tasks"],
      "properties": {
        "name": "massBundlePublish"
      }
    },
    {
      "id": "mass-image-push",
      "type": "ms.vss-distributed-task.task",
      "targets": ["ms.vss-distributed-task.tasks"],
      "properties": {
        "name": "massImagePush"
      }
    },
    {
      "id": "mass-app-patch",
      "type": "ms.vss-distributed-task.task",
      "targets": ["ms.vss-distributed-task.tasks"],
      "properties": {
        "name": "massAppPatch"
      }
    },
    {
      "id": "mass-app-deploy",
      "type": "ms.vss-distributed-task.task",
      "targets": ["ms.vss-distributed-task.tasks"],
      "properties": {
        "name": "massAppDeploy"
      }
    }
  ]
}
